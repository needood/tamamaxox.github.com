<div class="navigation clearfix">
  <a href='#<%= [ 'tree', state.branch].join('/') %>' class='title'><span>Prose</span></a>
  <a href="/{{site.baseurl}}#tree/master/_posts">文章列表</a>
  <a href="/{{site.baseurl}}#new/master/_posts">发表新文章</a>

  <% if (state.repo) { %>
    <% var path = state.path %>
  <% } else { %>
    <%= app.state.title %>
  <% } %>

  <div class='user-status<%= !window.authenticated ? " logged-out" : "" %>'>
  <% if (window.authenticated) { %>
    <%= app.username %> <a class="logout" href="#">Logout</a>
  <% } else { %>
      <a class="button" href="https://github.com/login/oauth/authorize?client_id={{site.oauth_client_id}}&scope=repo,user,gist&redirect_uri=<%= encodeURIComponent(window.location.href) %>">Sign in using Github</a>
  <% } %>
  </div>
  <% if (state.path) { %>
    <a class="browse-files" href="#<%= [ 'tree', state.branch, _.parentPath(state.path)].join('/') %>">
      Browse Files
    </a>
  <% } %>
</div>
